<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mic.dao.register.TeacherRegisterDao">

	<!-- 老师名称录入 -->
	<insert id="TeacherRegister"  parameterType="com.mic.bean.teacher.TeacherRegister" flushCache="true">  
			INSERT INTO teacher(
				username,
				password,
				email,
				address,
				phone
			)VALUES(
				#{username},
				#{password},
				#{email},
				#{address},
				#{phone}
			);
	</insert>  
</mapper>  